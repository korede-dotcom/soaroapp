	<script>
		const btn = document.querySelector("#saveChanges")
		btn.addEventListener("click",async (e) => {
		// form.addEventListener("submit", async (e) => {
				e.preventDefault();
				this.innerText = "Processing...";
				document.getElementById('loaderOverlay').style.display = 'flex';
				const form = document.querySelector("#payForm")
				const fordata = document.querySelector(".fordata")
				const formData = new FormData(form);
				const formObject = {};
	
				formData.forEach((value, key) => {
					formObject[key] = value;
				});
				console.log("🚀 ~ //form.addEventListener ~ formObject:", formObject)
				const end = document.querySelector("#end").value
				const start = document.querySelector("#start").value
				try {
					const id = "<%=tenants.id%>"
					const response = await fetch(`/tenant?type=pay`, {
						method: "POST",
						headers: {
							"Content-Type": "application/json",
						},
						body: JSON.stringify({end,start,roomId:fordata.getAttribute("data-room-id"),propertyId:fordata.getAttribute("data-property-id"),tenantId:fordata.getAttribute("data-tenant-id")}),
					});
					const data =  await response.json();
					console.log("🚀 ~ form.addEventListener ~ data:", data)
	
					if (data.status) {
						document.getElementById('loaderOverlay').style.display = 'none';
						Swal.fire({
							icon: "success",
							title: "Success!",
							text: data.message,
						});
						form.reset();
						window.location.reload()
					} else {
						document.getElementById('loaderOverlay').style.display = 'none';
						Swal.fire({
							icon: "error",
							title: "Submission Failed",
							text: data.message,
						});
					}
				} catch (error) {
					document.getElementById('loaderOverlay').style.display = 'none';
					console.error("Form submission error:", error);
					Swal.fire({
						icon: "error",
						title: "Error",
						text: error.toString(),
					});
				}
		})
	</script>
	<script>
